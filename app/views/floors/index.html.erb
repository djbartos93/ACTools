<p id="notice"><%= notice %></p>

<h1>Floors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th>Vfx</th>
      <th>Diy</th>
      <th>Buy</th>
      <th>Sell</th>
      <th>Color 1</th>
      <th>Color 2</th>
      <th>Source</th>
      <th>Source notes</th>
      <th>Version</th>
      <th>Hha concept 1</th>
      <th>Hha concept 2</th>
      <th>Hha series</th>
      <th>Tag</th>
      <th>Catalog</th>
      <th>Filename</th>
      <th>Internal</th>
      <th>Unieque entry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @floors.each do |floor| %>
      <tr>
        <td><%= floor.name %></td>
        <td><%= floor.image %></td>
        <td><%= floor.vfx %></td>
        <td><%= floor.diy %></td>
        <td><%= floor.buy %></td>
        <td><%= floor.sell %></td>
        <td><%= floor.color_1 %></td>
        <td><%= floor.color_2 %></td>
        <td><%= floor.source %></td>
        <td><%= floor.source_notes %></td>
        <td><%= floor.version %></td>
        <td><%= floor.hha_concept_1 %></td>
        <td><%= floor.hha_concept_2 %></td>
        <td><%= floor.hha_series %></td>
        <td><%= floor.tag %></td>
        <td><%= floor.catalog %></td>
        <td><%= floor.filename %></td>
        <td><%= floor.internal_id %></td>
        <td><%= floor.unieque_entry_id %></td>
        <td><%= link_to 'Show', floor %></td>
        <td><%= link_to 'Edit', edit_floor_path(floor) %></td>
        <td><%= link_to 'Destroy', floor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Floor', new_floor_path %>
